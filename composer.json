{
    "name": "appfromlab/bob",
    "description": "Composer tool to build WordPress plugin.",
    "type": "composer-plugin",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Appfromlab",
            "homepage": "https://www.appfromlab.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Appfromlab\\Bob\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "class": "Appfromlab\\Bob\\Composer\\BobPlugin"
    },
    "require": {
        "composer-plugin-api": "^2.9.0",
        "symfony/process": "^8.0.5"
    },
    "require-dev": {
        "composer/composer": "2.9.5"
    }
}
